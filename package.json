{
  "name": "express-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsr generate && webpack --mode production",
    "start": "cross-env DEBUG=express-server:* NODE_ENV=production node ./bin/www",
    "dev:server": "cross-env DEBUG=express-server:* NODE_ENV=development nodemon ./bin/www",
    "dev:client": "concurrently \"tsr watch\" \"webpack --mode development --watch\"",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write ."
  },
